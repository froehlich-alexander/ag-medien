{
  "compileOnSave": true,
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "jsx",
    "baseUrl": "./",
    "types": [
      "jquery",
      "./jsxFactory.ts"
    ],
    "module": "ES2020",
    "target": "ES6",
    "moduleResolution": "node",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "useDefineForClassFields": true,
    "sourceMap": true,
    "removeComments": false
  }
}