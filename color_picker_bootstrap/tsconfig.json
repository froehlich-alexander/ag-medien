{
  "compileOnSave": true,
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "types": [
      "jquery",
      "bootstrap",
      "react"
    ],
    "module": "ES2022",
    "target": "ES6",
    "moduleResolution": "node",
    "lib": [
      "ESNext",
      "DOM",
      "dom"
    ],
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "useDefineForClassFields": true,
    "sourceMap": true,
    "removeComments": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
  },
  "exclude": [
    "jsxFactory.ts"
  ]
}